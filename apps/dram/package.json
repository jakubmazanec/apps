{
  "name": "dram",
  "version": "0.5.2",
  "private": true,
  "description": "Whisky tasting notes app.",
  "repository": {
    "type": "git",
    "url": "git@github.com:jakubmazanec/apps.git",
    "directory": "apps/dram"
  },
  "license": "AGPL-3.0-only",
  "author": "Jakub Mazanec <jakub@mazanec.dev>",
  "type": "module",
  "scripts": {
    "build": "del-cli build && react-router build",
    "clean": "del-cli coverage build *.tsbuildinfo",
    "codegen": "react-router typegen && del-cli dbschema/edgeql-js dbschema/interfaces.* && generate edgeql-js --force-overwrite --target mts && generate interfaces --force-overwrite",
    "deploy": "flyctl deploy ../.. --config apps/dram/fly.toml",
    "develop": "react-router dev --port 5000",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "migrate": "edgedb migration create && edgedb migrate && npm run codegen",
    "start": "cross-env NODE_ENV=production PORT=5000 react-router-serve ./build/server/index.js",
    "test": "del-cli coverage && vitest run --coverage --passWithNoTests",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "dependencies": {
    "@edgedb/auth-remix": "^0.2.2",
    "@jakubmazanec/ui": "0.2.0",
    "@react-router/express": "^7.1.1",
    "@react-router/fs-routes": "^7.1.1",
    "@react-router/node": "^7.1.1",
    "@react-router/serve": "^7.1.1",
    "cross-env": "^7.0.3",
    "edgedb": "^1.5.12",
    "isbot": "^5.1.21",
    "lodash": "^4.17.21",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "react-router": "^7.1.1",
    "zod": "^3.24.1"
  },
  "devDependencies": {
    "@edgedb/generate": "^0.5.6",
    "@jakubmazanec/eslint-config": "^4.1.3",
    "@react-router/dev": "^7.1.1",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.1.0",
    "@types/fs-extra": "^11.0.4",
    "@types/lodash": "^4.17.14",
    "@types/node": "^22.0.0",
    "@types/react": "^19.0.7",
    "@types/react-dom": "^19.0.3",
    "@vitest/coverage-v8": "^2.1.8",
    "autoprefixer": "^10.4.20",
    "del-cli": "^6.0.0",
    "eslint": "^9.18.0",
    "fs-extra": "^11.3.0",
    "happy-dom": "^16.6.0",
    "p-map": "^7.0.3",
    "prettier": "^3.4.2",
    "prettier-plugin-packagejson": "^2.5.7",
    "prettier-plugin-tailwindcss": "^0.6.10",
    "tailwindcss": "^3.4.17",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3",
    "vite": "^6.0.9",
    "vitest": "^2.1.8",
    "xlsx": "^0.18.5",
    "zod": "^3.24.1"
  },
  "packageManager": "npm@10.9.0",
  "engines": {
    "node": "^22.13.0"
  }
}
