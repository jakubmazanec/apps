{
  "name": "somewhere",
  "version": "0.1.4",
  "private": true,
  "description": "Location-based game.",
  "repository": {
    "type": "git",
    "url": "git@github.com:jakubmazanec/apps.git",
    "directory": "apps/somewhere"
  },
  "license": "AGPL-3.0-only",
  "author": "Jakub Mazanec <jakub@mazanec.dev>",
  "type": "module",
  "scripts": {
    "build": "del-cli build && remix vite:build",
    "clean": "del-cli coverage build *.tsbuildinfo",
    "deploy": "flyctl deploy ../.. --config apps/somewhere/fly.toml",
    "develop": "remix vite:dev --port 5000",
    "format": "prettier . --write --ignore-unknown",
    "lint": "eslint .",
    "start": "cross-env NODE_ENV=production PORT=5000 remix-serve ./build/server/index.js",
    "test": "del-cli coverage && vitest run --coverage --passWithNoTests",
    "typecheck": "tsc --project tsconfig.typecheck.json"
  },
  "dependencies": {
    "@jakubmazanec/ts-utils": "^2.1.2",
    "@jakubmazanec/zod-utils": "^1.0.2",
    "@remix-run/express": "^2.8.1",
    "@remix-run/node": "^2.8.1",
    "@remix-run/react": "^2.8.1",
    "@remix-run/serve": "^2.8.1",
    "cross-env": "^7.0.3",
    "eventemitter3": "^5.0.1",
    "isbot": "^5.1.3",
    "lodash": "^4.17.21",
    "pixi-filters": "^5.3.0",
    "pixi.js": "^7.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@jakubmazanec/eslint-config": "^4.0.0",
    "@remix-run/dev": "^2.8.1",
    "@testing-library/jest-dom": "^6.4.2",
    "@testing-library/react": "^16.0.0",
    "@types/lodash": "^4.17.0",
    "@types/node": "^20.12.2",
    "@types/react": "^18.2.73",
    "@types/react-dom": "^18.2.23",
    "@vitest/coverage-v8": "^2.0.0",
    "autoprefixer": "^10.4.19",
    "del-cli": "^6.0.0",
    "eslint": "^9.0.0",
    "happy-dom": "^15.0.0",
    "prettier": "^3.2.5",
    "prettier-plugin-packagejson": "^2.4.14",
    "prettier-plugin-tailwindcss": "^0.5.13",
    "tailwindcss": "^3.4.3",
    "typescript": "^5.7.2",
    "vite": "^5.2.7",
    "vitest": "^2.0.0"
  },
  "packageManager": "npm@10.9.0",
  "engines": {
    "node": "^22.11.0"
  }
}
